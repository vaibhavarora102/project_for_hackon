covid=0
typhoid=0
chicken_pox=0
pneumonia=0
influenza=0
sars=0
mers=0
ebola=0
malaria=0

##############--------------------# set yes, no and may be as input to to each symptomm
query=input()


#---------------------------- and follow following


#------------------in case of fever
if query=='yes':
    covid += 1
    typhoid += 1
    chicken_pox += 1
    pneumonia += 1
    influenza += 1
    sars += 1
    mers += 1
    ebola += 1
    malaria +=1

elif query=='no':
    covid -= 1
    typhoid -= 1
    chicken_pox -= 1
    pneumonia -= 1
    influenza -= 1
    sars -= 1
    mers -= 1
    ebola -= 1
    malaria -= 1

